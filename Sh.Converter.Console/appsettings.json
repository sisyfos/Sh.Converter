{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Settings": {
    "SourceFilePath": "source\\people.txt",
    "SourceSeparator": "|",
    "OutputDocumentRoot": "People"
  },
  "Relation": [
    {
      "Designator": "P",
      "Type": "Person",
      "Fields": [ "FirstName", "LastName" ],
      "IsRoot": true,
      "CanHaveChildren": [ "T", "A", "F" ]
    },
    {
      "Designator": "F",
      "Type": "Family",
      "Fields": [ "Name", "Born" ],
      "IsRoot": false,
      "CanHaveChildren": [ "T", "A" ]
    },
    {
      "Designator": "T",
      "Type": "Phone",
      "Fields": [ "Mobile", "LandLine" ],
      "IsRoot": false
    },
    {
      "Designator": "A",
      "Type": "Address",
      "Fields": [ "Street", "City", "PostalCode" ],
      "IsRoot": false
    }
  ]
}